{
  "name": "rs-node-js-simple-crud-app",
  "version": "1.0.0",
  "main": "./src/server.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js",
    "start:dev": "nodemon ./src/server.js",
    "start:prod": "cross-env npm run build:prod && npm run prod",
    "prod": "node ./dist/server.bundle.cjs",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "test:e2e": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .js,.cjs",
    "lint:fix": "eslint --fix --ext .js,.cjs",
    "build": "webpack",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lowton/rs-node-js-simple-crud-app.git"
  },
  "keywords": [
    "node",
    "crud",
    "jest"
  ],
  "author": "Lowton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lowton/rs-node-js-simple-crud-app/issues"
  },
  "homepage": "https://github.com/Lowton/rs-node-js-simple-crud-app#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@types/jest": "^27.0.3",
    "@webpack-cli/generators": "^1.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "dotenv": "^10.0.0",
    "uuid": "^8.3.2"
  }
}
